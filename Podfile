# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'


def main_pods
  pod 'SwiftyJSON'
  pod 'Alamofire'
  pod 'ObjectMapper'
  pod 'AWSCore'
  pod 'AWSCognitoIdentityProvider'
  pod 'SDWebImage'
  pod 'IQKeyboardManagerSwift'
  pod 'XMLMapper'
end

target 'DQHome-Dev' do
  use_frameworks!
  main_pods
end

target 'DQHome-Prod' do
    use_frameworks!
    main_pods
end
